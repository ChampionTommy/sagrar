{
  "name": "nextarter-chakra",
  "version": "0.1.0",
  "private": true,
  "author": "agustinusnathaniel",
  "engines": {
    "node": ">=22.16.x"
  },
  "packageManager": "pnpm@10.7.1",
  "pnpm": {
    "neverBuiltDependencies": []
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "turbo": "pnpm dlx turbo@2.5.0",
    "build:turbo": "pnpm turbo run build",
    "postbuild": "next-sitemap --config next-sitemap.config.js",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "eslint src --fix && pnpm format",
    "type-check": "tsc --noEmit",
    "check:turbo": "pnpm turbo lint type-check",
    "test:e2e": "playwright test",
    "format": "prettier --write src",
    "up-interactive": "pnpm up -i",
    "up-latest": "pnpm up-interactive -L",
    "release": "cross-env HUSKY=0 commit-and-tag-version",
    "push-release": "git push --follow-tags origin main",
    "prepare": "husky"
  },
  "dependencies": {
    "@radix-ui/react-accordion": "^1.2.11",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-menubar": "^1.1.15",
    "@radix-ui/react-navigation-menu": "^1.2.13",
    "@radix-ui/react-scroll-area": "^1.2.9",
    "@radix-ui/react-slot": "^1.2.3",
    "@tailwindcss/postcss": "^4.1.10",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "commit-and-tag-version": "^12.5.1",
    "embla-carousel-react": "^8.6.0",
    "knip": "^5.61.0",
    "lucide-react": "^0.514.0",
    "next": "^15.3.3",
    "next-themes": "^0.4.6",
    "postcss": "^8.5.5",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "sass": "^1.89.2",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "19.8.1",
    "@commitlint/cz-commitlint": "19.8.1",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.28.0",
    "@next/eslint-plugin-next": "^15.3.3",
    "@playwright/test": "^1.53.0",
    "@types/node": "22.15.29",
    "@types/react": "^19.1.8",
    "autoprefixer": "^10.0.0",
    "commitizen": "^4.3.1",
    "cross-env": "^7.0.3",
    "eslint": "^9.28.0",
    "eslint-config-next": "^15.3.3",
    "eslint-plugin-react-hooks": "^5.2.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.0",
    "next-sitemap": "^4.2.3",
    "prettier": "^3.5.3",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.8.3"
  }
}
